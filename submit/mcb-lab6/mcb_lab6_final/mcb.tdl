;;; Lexical rule types

noun-pc-loc-lex-rule-super := add-only-no-ccont-rule & noun-pc-del_nomz-rule-dtr &
  [ INFLECTED #infl,
    DTR noun-pc-loc-rule-dtr &
        [ INFLECTED #infl ] ].

